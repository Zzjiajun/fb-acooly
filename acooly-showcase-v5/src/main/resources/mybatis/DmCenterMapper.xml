<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.acooly.showcase.daliy.dao.DmCenterDao">

    <resultMap id="dmCenterResultMap" type="com.acooly.showcase.daliy.entity.DmCenter">
        <id property="id" column="dmCenter_id"/>
        <result property="userName" column="dmCenter_user_name"/>
        <result property="region" column="dmCenter_region"/>
        <result property="pixel" column="dmCenter_pixel"/>
        <result property="link" column="dmCenter_link"/>
        <result property="domain" column="dmCenter_domain"/>
        <result property="secondaryDomain" column="dmCenter_secondary_domain"/>
        <result property="serialNumber" column="dmCenter_serial_number"/>
        <result property="visitsNumber" column="dmCenter_visits_number"/>
