    <sql id="dmCenterSqlSelect">
            dmCenter.id as dmCenter_id,
            dmCenter.user_name as dmCenter_user_name,
            dmCenter.region as dmCenter_region,
            dmCenter.pixel as dmCenter_pixel,
            dmCenter.link as dmCenter_link,
            dmCenter.domain as dmCenter_domain,
            dmCenter.secondary_domain as dmCenter_secondary_domain,
            dmCenter.
serial_number as dmCenter_
serial_number,
            dmCenter.visits_number as dmCenter_visits_number,
            dmCenter.clicks_number as dmCenter_clicks_number,
            dmCenter.create_time as dmCenter_create_time,
            dmCenter.update_time as dmCenter_update_time
    </sql>

</mapper>